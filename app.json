{
  "expo": {
    "name": "ekTrackEz",
    "slug": "material-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/without.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/without.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE"
      ],
      "package": "com.zaidhassan71.materialtracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/images/small.png",
      "color": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "40696e80-5086-472e-a78b-1d80df32a002"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "zaidhassan71"
  }
}
